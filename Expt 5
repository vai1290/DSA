clc;
clear;
close;

count = csvRead("C:/Users/Tanishq/Documents/tav/count.dat");

disp("Data read successfully:");
disp(count);

for i = 1:3
    scf(i);
    mu(i) = mean(count(:, i));
    sigma(i) = stdev(count(:, i));
    histplot(10, count(:, i));
    xtitle("Histogram of Traffic Intersection " + string(i), "Traffic Count", "Frequency");
end

MeanTotal = mean(mean(count));
disp("Mean for individual Traffic Intersection is =", mu);
disp("Standard Deviation for individual Traffic Intersection is =", sigma);
disp("Overall Mean =", MeanTotal);
